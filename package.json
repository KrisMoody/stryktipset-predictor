{
  "name": "st-predictor",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nuxt dev",
    "dev:ai": "cd services/ai-scraper && ./start.sh",
    "dev:all": "concurrently -n \"nuxt,ai\" -c \"blue,green\" \"npm run dev\" \"npm run dev:ai\"",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && prisma generate",
    "prepare": "husky",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "nuxt typecheck",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:migrate:deploy": "prisma migrate deploy && supabase db push",
    "db:generate": "prisma generate",
    "db:supabase:push": "supabase db push",
    "db:supabase:pull": "supabase db pull",
    "vercel-build": "prisma generate && nuxt build",
    "backfill": "tsx scripts/backfill-season.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run tests/unit/",
    "test:components": "vitest run tests/components/",
    "test:api": "vitest run tests/api/",
    "test:integration": "vitest run tests/integration/",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:smoke": "playwright test tests/e2e/smoke.spec.ts",
    "test:a11y": "playwright test tests/accessibility/",
    "test:all": "vitest run && playwright test"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@bugsnag/js": "^8.6.0",
    "@bugsnag/plugin-vue": "^8.6.0",
    "@nuxt/icon": "^2.2.0",
    "@nuxt/ui": "^4.3.0",
    "@nuxtjs/supabase": "^2.0.3",
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "@supabase/supabase-js": "^2.87.1",
    "chart.js": "^4.4.1",
    "croner": "^9.1.0",
    "dotenv": "^17.2.3",
    "esbuild": "^0.27.2",
    "node-cache": "^5.1.2",
    "nuxt": "^4.2.2",
    "openai": "^6.10.0",
    "p-limit": "^7.2.0",
    "pg": "^8.16.3",
    "playwright": "^1.51.1",
    "vue": "^3.5.26"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@bugsnag/source-maps": "^2.3.3",
    "@faker-js/faker": "^10.1.0",
    "@iconify-json/heroicons": "^1.2.3",
    "@iconify-json/lucide": "^1.2.82",
    "@nuxt/eslint": "^1.12.1",
    "@nuxt/test-utils": "^3.21.0",
    "@playwright/test": "^1.57.0",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "@vue/compiler-sfc": "^3.5.26",
    "@vue/test-utils": "^2.4.6",
    "concurrently": "^9.2.1",
    "embla-carousel": "^8.6.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "happy-dom": "^20.0.11",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "msw": "^2.12.7",
    "prettier": "^3.7.4",
    "prisma": "^7.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.8.2",
    "vite": "^7.3.0",
    "vitest": "^4.0.16",
    "vitest-mock-extended": "^3.1.0",
    "vue-tsc": "^3.2.1"
  }
}
